    </tdml:document>
    <tdml:infoset>
      <tdml:dfdlInfoset>
        <vcard:vCards xmlns="http://www.ibm.com/dfdl/vCard/2.1" >
          <vCard>
            <Version xmlns="">
              <VersionNumber xsi:type="xs:string">2.1</VersionNumber>
            </Version>
            <Name xmlns="">
              <Family xsi:type="xs:string">Bloggs</Family>
              <Given xsi:type="xs:string">Joe</Given>
            </Name>
            <FormattedName xmlns="">
              <Value xsi:type="xs:string">Joe Bloggs</Value>
            </FormattedName>
            <DeliveryAddress xmlns="">
              <Parameter xsi:type="xs:string">HOME</Parameter>
              <Street xsi:type="xs:string">1 Trafalgar Square</Street>
              <Locality xsi:type="xs:string">London</Locality>
              <PostalCode xsi:type="xs:string">WC2N</PostalCode>
              <CountryName xsi:type="xs:string">United Kingdom</CountryName>
            </DeliveryAddress>
            <Agent xmlns="">
              <vCard>
                <Version>
                  <VersionNumber xsi:type="xs:string">2.1</VersionNumber>
                </Version>
                <Name>
                  <Family xsi:type="xs:string">Orange</Family>
                  <Given xsi:type="xs:string">Agent</Given>
